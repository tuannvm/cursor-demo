        activeAlerts.push({
          name: rule.name,
          severity: rule.severity,
          message: rule.message,
          timestamp: new Date(),
          value: this.extractMetricValue(metrics, rule.name)
        });
      }
    }
    
    return activeAlerts;
  }
}
